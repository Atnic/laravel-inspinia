{
    "name": "atnic/laravel-inspinia",
    "description": "Laravel Package for integrating Inspinia template and this package is Laravel Mix friendly.",
    "keywords": ["laravel", "inspinia", "package", "composer"],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "laravel/tinker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Atnic\\Inspinia\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atnic\\Inspinia\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atnic\\Inspinia\\Providers\\AppServiceProvider"
            ]
        }
    }
}
